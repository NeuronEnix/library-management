<% if( typeof eleKeyValPair !== "undefined" ) { %>
    <script type="text/javascript">
        var eleKeyValPair = <%-JSON.stringify(eleKeyValPair)%>;
        for( const key in eleKeyValPair ) {
            document.getElementById( key ).value = eleKeyValPair[ key ];
            
        }
    </script>        
        
    <% } %> 