
    <link href="/css/table.css" rel="stylesheet" type="text/css">
<table class="table table-striped table-border tableFixHead table-responsive"  style="text-align:center;">
    <!-- table table-striped table-bordered  -->
    <thead>
        <tr>
            <th >#</th>
            <% headerList.forEach( header => { %>
                <th><%= header %></th>
            <% }) %>
        </tr>
    </thead>
    <tbody>
        
        <% 
        try {
            if ( rowList.length == 0 ) throw "";
            rowList.forEach( row => { %>
                <tr class="bg-<%= row.color %>">
                    <th> <%= startSlNum++ %></th>
                    <% rowListKeys.forEach( key => { %>
                        <td><%= row[ key ] %></td>
                    <% }) %>
                </tr>
            
        <% }) %>


        <% } catch ( err ) { %>
            <h2>No History</h2>
        <% } %>

    </tbody>  
</table>