
    <link href="/css/table.css" rel="stylesheet" type="text/css">
<table id="dtDynamicVerticalScrollExample" class="table table-striped table-bordered tableFixHead table-responsive" style="text-align:center; width: 100%">
    <!-- table table-striped table-bordered  -->
    <thead>
      <tr>
          <th >#</th>
          <% headerList.forEach( header => { %>
              <th><%= header %></th>
          <% }) %>
      </tr>
    </thead>
    <tbody>
        
        <% 
        try {
            rowList.forEach( row => { %>

            <tr class="bg-<%= row.color %>">
              <th> <%= startSlNum++ %></th>
                <% rowListKeys.forEach( key => { %>
                    <td><%= row[ key ] %></td>
                <% }) %>
            </tr>
            
        <% }) %>


        <% } catch ( err ) { %>
            <h2>No History</h2>
        <% } %>

    </tbody>  
</table>