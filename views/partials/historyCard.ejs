<div class="card-header py-3">
    <h6 class="m-0 font-weight-bold text-primary">History</h6>
</div>
<div class="card-body" style="width: 100%; padding:0px">
    <form action="<%= refreshAction %>" method="get">
    <%- include( "../partials/hiddenField", { hiddenFieldList } ) %>
    <% checkBoxList.forEach( checkBox => {

            const { name, id, label, color, checked=true } = checkBox %>
            
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="checkbox" name="<%= name %>" id="<%= id %>">
                <label class="form-check-label text-<%= color %>" for="<%= id %>"><%= label %></label>
            </div>
            <script type="text/javascript">
                if ( "<%= checked %>" == "true")
                    document.getElementById( "<%= id %>").checked = true;
            </script>
        <% }) %>
        <div class="form-check form-check-inline">
            <button type="submit" class="btn btn-primary">Refresh</button>
        </div>
    </form>
    <div class="row justify-content-center" style="width: 100%" ><%- include( "../partials/table", { ...table } ) %></div>
    
    <div class="row justify-content-center" style="width: 100%" >


    </div>
</div>